# Ejemplo de variables de entorno para la aplicación cliente
# Copia este archivo a .env.production y configura los valores

# URL del backend API
VITE_API_URL=https://tu-backend.onrender.com/arye_system

# Entorno de la aplicación
VITE_APP_ENV=production

# Nombre de la aplicación
VITE_APP_NAME=AR-E-Web Client

# Habilitar analytics (opcional)
VITE_ENABLE_ANALYTICS=true

# Debug mode (para producción debe ser false)
VITE_DEBUG=false

# Target de deploy (para builds específicos)
VITE_DEPLOY_TARGET=vercel
